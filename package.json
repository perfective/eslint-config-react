{
    "name": "@perfective/eslint-config-react",
    "version": "0.5.0",
    "description": "ESLint shareable rules configuration for React",
    "keywords": [
        "react",
        "code quality",
        "code standard",
        "code style",
        "eslint",
        "eslint-config",
        "lint",
        "perfective",
        "typescript"
    ],
    "author": "Andrey Mikheychik <a.mikheychik@gmail.com>",
    "homepage": "https://github.com/perfective/eslint-config-react",
    "repository": {
        "type": "git",
        "url": "https://github.com/perfective/eslint-config-react.git"
    },
    "license": "MIT",
    "scripts": {
        "dist": "gulp",
        "lint": "npm run lint:eslint && npm run lint:prettier",
        "lint:eslint": "eslint --fix ./",
        "lint:prettier": "prettier --write .",
        "lint:prettier:build": "prettier --check .",
        "test": "jest",
        "test:build": "jest --clearCache && jest --collectCoverage"
    },
    "dependencies": {
        "@perfective/eslint-config": "~0.15.0"
    },
    "peerDependencies": {
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-react-hooks-ssr": "^0.1.5",
        "eslint-plugin-react-perf": "^3.3.0"
    },
    "devDependencies": {
        "@asciidoctor/gulp-asciidoctor": "^2.2.5",
        "@babel/eslint-parser": "^7.14.7",
        "@perfective/build": "~0.1.2",
        "@types/jest": "^27.0.1",
        "@typescript-eslint/eslint-plugin": "4.28.0",
        "@typescript-eslint/eslint-plugin-tslint": "4.28.0",
        "@typescript-eslint/parser": "4.28.0",
        "eslint": "7.29.0",
        "eslint-import-resolver-typescript": "2.4.0",
        "eslint-plugin-array-func": "3.1.7",
        "eslint-plugin-cypress": "2.11.3",
        "eslint-plugin-deprecation": "1.2.1",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.23.4",
        "eslint-plugin-jest": "24.3.6",
        "eslint-plugin-jest-dom": "3.9.0",
        "eslint-plugin-jest-formatting": "3.0.0",
        "eslint-plugin-jsdoc": "35.4.0",
        "eslint-plugin-jsx-a11y": "6.4.1",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "5.1.0",
        "eslint-plugin-react": "7.24.0",
        "eslint-plugin-react-hooks": "4.2.0",
        "eslint-plugin-react-hooks-ssr": "0.1.5",
        "eslint-plugin-react-perf": "3.3.0",
        "eslint-plugin-rxjs": "3.3.3",
        "eslint-plugin-simple-import-sort": "7.0.0",
        "eslint-plugin-sonarjs": "0.7.0",
        "eslint-plugin-testing-library": "4.6.0",
        "eslint-plugin-unicorn": "33.0.1",
        "gulp": "^4.0.2",
        "jest": "^27.1.0",
        "prettier": "^2.3.1",
        "ts-jest": "^27.0.5",
        "tslint": "6.1.3",
        "typescript": "~4.3.4"
    }
