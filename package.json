{
    "name": "@perfective/eslint-config-react",
    "version": "0.18.0",
    "description": "ESLint shareable rules configuration for React",
    "keywords": [
        "react",
        "code quality",
        "code standard",
        "code style",
        "eslint",
        "eslint-config",
        "lint",
        "perfective",
        "typescript"
    ],
    "author": "Andrey Mikheychik <a.mikheychik@gmail.com>",
    "homepage": "https://github.com/perfective/eslint-config-react",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/perfective/eslint-config-react.git"
    },
    "type": "module",
    "license": "MIT",
    "scripts": {
        "dist": "gulp",
        "lint": "gulp && npm run lint:eslint && npm run lint:prettier",
        "lint:build": "gulp && npm run lint:eslint:build && npm run lint:prettier:build",
        "lint:eslint": "eslint --fix ./",
        "lint:eslint:build": "eslint --max-warnings 0 ./",
        "lint:prettier": "prettier --write .",
        "lint:prettier:build": "prettier --check .",
        "test": "gulp spec && NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest",
        "test:build": "gulp spec && NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --clearCache && NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --collectCoverage"
    },
    "dependencies": {
        "@perfective/eslint-config": "~0.34.1"
    },
    "peerDependencies": {
        "eslint": "^9.39.2",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^7.0.1",
        "eslint-plugin-react-perf": "^3.3.3",
        "globals": "^17.0.0"
    },
    "devDependencies": {
        "@asciidoctor/gulp-asciidoctor": "^2.2.5",
        "@jest/globals": "^30.2.0",
        "@eslint-community/eslint-plugin-eslint-comments": "4.5.0",
        "@perfective/build": "~0.22.0",
        "@stylistic/eslint-plugin": "5.6.1",
        "@types/jest": "^30.0.0",
        "eslint": "9.39.2",
        "eslint-import-resolver-typescript": "3.10.1",
        "eslint-plugin-array-func": "5.1.0",
        "eslint-plugin-cypress": "5.2.1",
        "eslint-plugin-import": "2.32.0",
        "eslint-plugin-jest": "29.12.1",
        "eslint-plugin-jest-dom": "5.5.0",
        "eslint-plugin-jsdoc": "62.0.0",
        "eslint-plugin-jsx-a11y": "6.10.2",
        "eslint-plugin-n": "17.23.1",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "7.2.1",
        "eslint-plugin-react": "7.37.2",
        "eslint-plugin-react-hooks": "7.0.1",
        "eslint-plugin-react-perf": "3.3.3",
        "eslint-plugin-rxjs-x": "0.8.4",
        "eslint-plugin-security": "3.0.1",
        "eslint-plugin-simple-import-sort": "12.1.1",
        "eslint-plugin-testing-library": "7.15.4",
        "eslint-plugin-unicorn": "62.0.0",
        "globals": "17.0.0",
        "gulp": "^5.0.1",
        "jest": "^30.2.0",
        "prettier": "^3.7.4",
        "ts-jest": "^29.4.6",
        "typescript": "~5.9.3",
        "typescript-eslint": "8.52.0"
    }
}
