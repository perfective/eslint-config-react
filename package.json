{
    "name": "@perfective/eslint-config-react",
    "version": "0.10.0",
    "description": "ESLint shareable rules configuration for React",
    "keywords": [
        "react",
        "code quality",
        "code standard",
        "code style",
        "eslint",
        "eslint-config",
        "lint",
        "perfective",
        "typescript"
    ],
    "author": "Andrey Mikheychik <a.mikheychik@gmail.com>",
    "homepage": "https://github.com/perfective/eslint-config-react",
    "repository": {
        "type": "git",
        "url": "https://github.com/perfective/eslint-config-react.git"
    },
    "license": "MIT",
    "scripts": {
        "dist": "gulp",
        "lint": "npm run lint:eslint && npm run lint:prettier",
        "lint:eslint": "eslint --fix ./",
        "lint:prettier": "prettier --write .",
        "lint:prettier:build": "prettier --check .",
        "test": "jest",
        "test:build": "jest --clearCache && jest --collectCoverage"
    },
    "dependencies": {
        "@perfective/eslint-config": "~0.21.0-beta"
    },
    "peerDependencies": {
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-hooks-ssr": "^0.1.5",
        "eslint-plugin-react-perf": "^3.3.1"
    },
    "devDependencies": {
        "@asciidoctor/gulp-asciidoctor": "^2.2.5",
        "@babel/eslint-parser": "^7.19.1",
        "@perfective/build": "~0.7.4",
        "@types/jest": "^29.5.0",
        "@typescript-eslint/eslint-plugin": "5.59.1",
        "@typescript-eslint/eslint-plugin-tslint": "5.59.1",
        "@typescript-eslint/parser": "5.59.1",
        "eslint": "8.39.0",
        "eslint-import-resolver-typescript": "3.5.5",
        "eslint-plugin-array-func": "3.1.8",
        "eslint-plugin-cypress": "2.13.3",
        "eslint-plugin-deprecation": "1.4.1",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jest": "27.2.1",
        "eslint-plugin-jest-dom": "4.0.3",
        "eslint-plugin-jest-formatting": "3.1.0",
        "eslint-plugin-jsdoc": "43.1.1",
        "eslint-plugin-jsx-a11y": "6.7.1",
        "eslint-plugin-node": "11.1.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "6.1.1",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "eslint-plugin-react-hooks-ssr": "0.1.5",
        "eslint-plugin-react-perf": "3.3.1",
        "eslint-plugin-rxjs": "5.0.3",
        "eslint-plugin-simple-import-sort": "10.0.0",
        "eslint-plugin-sonarjs": "0.19.0",
        "eslint-plugin-testing-library": "5.10.3",
        "eslint-plugin-unicorn": "46.0.0",
        "gulp": "^4.0.2",
        "jest": "^29.5.0",
        "prettier": "~2.8.4",
        "ts-jest": "^29.0.5",
        "tslint": "6.1.3",
        "typescript": "~4.9.5"
    },
    "overrides": {
        "glob-parent": "^5.1.2"
    }
}
