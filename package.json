{
    "name": "@perfective/eslint-config-react",
    "version": "0.10.0-alpha",
    "description": "ESLint shareable rules configuration for React",
    "keywords": [
        "react",
        "code quality",
        "code standard",
        "code style",
        "eslint",
        "eslint-config",
        "lint",
        "perfective",
        "typescript"
    ],
    "author": "Andrey Mikheychik <a.mikheychik@gmail.com>",
    "homepage": "https://github.com/perfective/eslint-config-react",
    "repository": {
        "type": "git",
        "url": "https://github.com/perfective/eslint-config-react.git"
    },
    "license": "MIT",
    "scripts": {
        "dist": "gulp",
        "lint": "npm run lint:eslint && npm run lint:prettier",
        "lint:eslint": "eslint --fix ./",
        "lint:prettier": "prettier --write .",
        "lint:prettier:build": "prettier --check .",
        "test": "jest",
        "test:build": "jest --clearCache && jest --collectCoverage"
    },
    "dependencies": {
        "@perfective/eslint-config": "~0.20.0-alpha.1"
    },
    "peerDependencies": {
        "eslint-plugin-jsx-a11y": "^6.6.1",
